#include <stdio.h>
#include <math.h>

int main()
{
    int n, i;
    int sum = 0;

    printf("Enter the number of terms: ");
    scanf("%d", &n);

    for (i = 1; i <= n; i++)
    {
        sum = sum + pow(i, 3);
    }

    printf("The sum of the cubes of the first %d numbers is: %d", n, sum);

    return 0;
}
